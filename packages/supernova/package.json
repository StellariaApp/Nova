{
  "name": "@stellaria/supernova",
  "version": "0.3.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/taishinaritomi/excss.git"
  },
  "license": "MIT",
  "author": "William Jesus (Willishakespeare)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    },
    "./compiler": {
      "types": "./dist/compiler.d.ts",
      "import": "./dist/compiler.mjs",
      "require": "./dist/compiler.cjs"
    },
    "./vite": {
      "types": "./dist/plugins/vite.d.ts",
      "import": "./dist/plugins/vite.mjs",
      "require": "./dist/plugins/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/plugins/webpack/plugin.d.ts",
      "import": "./dist/plugins/webpack/plugin.mjs",
      "require": "./dist/plugins/webpack/plugin.cjs"
    },
    "./webpack/loader": {
      "types": "./dist/plugins/webpack/loader.d.ts",
      "import": "./dist/plugins/webpack/loader.mjs",
      "require": "./dist/plugins/webpack/loader.cjs"
    },
    "./webpack/cssLoader": {
      "types": "./dist/plugins/webpack/cssLoader.d.ts",
      "import": "./dist/plugins/webpack/cssLoader.mjs",
      "require": "./dist/plugins/webpack/cssLoader.cjs"
    },
    "./next": {
      "types": "./dist/plugins/next.d.ts",
      "import": "./dist/plugins/next.mjs",
      "require": "./dist/plugins/next.cjs"
    },
    "./assets/*": "./src/assets/*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "config": [
        "./dist/core/config"
      ],
      "compiler": [
        "./dist/core/compiler"
      ],
      "vite": [
        "./dist/plugins/vite"
      ],
      "webpack": [
        "./dist/plugins/webpack/plugin"
      ],
      "webpack/loader": [
        "./dist/plugins/webpack/loader"
      ],
      "webpack/cssLoader": [
        "./dist/plugins/webpack/cssLoader"
      ],
      "next": [
        "./dist/plugins/next"
      ]
    }
  },
  "files": [
    "dist",
    "assets",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsx ./src/scripts/build.ts",
    "dev": "tsx watch ./src/scripts/build.ts"
  },
  "dependencies": {
    "esbuild": "^0.19.2"
  },
  "devDependencies": {
    "next": "^14.0.1",
    "tsup": "^5.10.1",
    "vite": "^4.4.4",
    "webpack": "^5.88.1"
  },
  "peerDependencies": {
    "next": "^14",
    "vite": "^4",
    "webpack": "^5"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  }
}
